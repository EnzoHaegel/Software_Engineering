{
    "manifest_version": 3,
    "name": "CourseMate",
    "description": "Manage your NTPU schedule easily!",
    "version": "1.1",
    "permissions": [
      "cookies",
      "tabs",
      "scripting",
      "activeTab"
    ],
    "action": {
        "default_popup": "index.html",
        "default_icon": "assets/logo.png"
    }
}
